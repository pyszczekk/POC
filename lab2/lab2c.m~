close all;
img1 = imread('parrot.bmp');
img2 = imread('chessboard.bmp');
img3 = imread('clock.bmp');

figure();
imshow(img1);

figure()
imshow(imresize(img1, 2, 'nearest'));
title('sasiada');
figure()
imshow(imresize(img1, 1.3, 'bilinear'));

figure();
imshow(img2);

figure()
imshow(imresize(img2, 2));
figure()
imshow(imresize(img2, 2.3));



figure();
imshow(img3);

figure()
imshow(imresize(img3, 2));
figure()
imshow(imresize(img3, 1.8));