close all;
clearvars;
clc;

img = imread('dom.png');

% log

figure(1);
subplot(1,2,1)
imshow(img)
title('oryginal');
subplot(1,2,2);
imshow(edge(img, 'log'))
title('detekcja krawedzi - log');


% canny

figure(2);
subplot(1,2,1)
imshow(img)
title('oryginal');
subplot(1,2,2);
imshow(edge(img, 'canny',[0.01,0.07]))
title('detekcja krawedzi - canny');

%dobor reczny
figure(3)
subplot(3,2,1);
imshow()